{
  "name": "arrilot/laravel-data-anonymization",
  "license": "MIT",
  "description": "Laravel bridge for arrilot/data-anonymization",
  "keywords": [
    "anonymization",
    "database",
    "laravel"
  ],
  "authors": [
    {
      "name": "Nekrasov Ilya",
      "email": "nekrasov.ilya90@gmail.com"
    }
  ],
  "homepage": "https://github.com/arrilot/laravel-data-anonymization",
  "require": {
    "php": ">=5.5.9",
    "arrilot/data-anonymization": "~1.0",
    "illuminate/support": ">=5.5",
    "illuminate/contracts": ">=5.5",
    "illuminate/console": ">=5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "~0.9"
  },
  "autoload": {
    "psr-4": {
      "Arrilot\\LaravelDataAnonymization\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Arrilot\\Tests\\LaravelDataAnonymization\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Arrilot\\LaravelDataAnonymization\\ServiceProvider"
      ]
    }
  }
}
